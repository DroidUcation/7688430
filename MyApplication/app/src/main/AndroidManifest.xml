<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goldstein.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.FirstActivity" />
        <activity android:name=".Activities.SecondActivity" />
        <activity android:name=".Activities.ThirdActivity" />
        <activity android:name=".Activities.ForthActivity" />
        <activity android:name=".Activities.FifthActivity" />
        <provider
            android:authorities="com.example.goldstein.myapplication"
            android:name=".Providers.CameraProvider"
            >
        </provider>

</application>
</manifest>
